{
    "compilerOptions": {
      "baseUrl": "./src",
      "paths": {
        "*": ["*"]
      },
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "exclude": ["node_modules"]
  }
  